{
  "title": "domain.extension",
  "language": {
    "iso": {
      "code": "en",
      "name": "English"
    }
  },
  "url": {
    "logo": "https://cdn.jsdelivr.net/gh/CSOIreland/PxStat/client/image/logo.png",
    "jsonrpc": "https://webserice.domain.extension/public/api.jsonrpc",
    "restful": "https://webserice.domain.extension/public/api.restful/",
    "application": "https://domain.extension/"
  },
  "dataset": {
    "officialStatistics": true,
    "download": {
      "threshold": {
        "csv": 1048575,
        "xlsx": 1048575
      }
    }
  },

  "regex": {
    "phone": {
      "pattern": "^(\\(\\+\\d{1,3}\\) \\d+ \\d+ \\d+)$",
      "placeholder": "(+353) 21 452 2253"

    },
    "password": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[\\W]).{8,}$"
  },
  "workflow": {
    "embargo": {
      "time": "11:00:00",
      "day": [
        1,
        2,
        3,
        4,
        5
      ]
    },
    "fastrack": {
      "response": {
        "approver": true,
        "poweruser": true,
        "administrator": false
      },
      "signoff": {
        "poweruser": true,
        "administrator": false
      }
    }
  },
  "build": {
    "create": {
      "moderator": true
    },
    "update": {
      "moderator": true
    },
    "import": {
      "moderator": true
    }
  },
  "session": {
    "length": 600
  },
  "search": {
    "maximum-results": 100
  }
}
